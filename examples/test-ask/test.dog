
DEFINE label FOR people ON image DO
  PERFORM "Please label these images"
  RETURN label
END

labels = ASK people.public TO label ON "Foobar"

PRINT "before"

WAIT ON labels

PRINT "after"

PRINT labels
PRINT labels[0].label

PRINT  "Labels Person: "
PRINT PERSON FROM labels

PRINT  "Labels[0] Person: "
PRINT PERSON FROM labels[0]


PRINT "All Done:"
PRINT labels